{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "kyee_nextframework_portalface_quickstart"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "kyee_nextframework_portalface_quickstart",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "tsconfig": "tsconfig.app.json",
      "prefix": "kyee",
      "styles": [
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/portalface/styles/theme.scss"
      ],
      "scripts": [
        
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "component": {
      "spec": false
    },
    "directive": {
      "spec": false
    },
    "guard": {
      "spec": false
    },
    "interface": {
    },
    "module": {
      "spec": false
    },
    "pipe": {
      "spec": false
    },
    "service": {
      "spec": false
    },
    "serve": {
      "port": 2060
    }
  }
}
